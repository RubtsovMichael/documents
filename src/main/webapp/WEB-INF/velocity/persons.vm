<!DOCTYPE html>
<html>
<head>
    #include("common/styles_import.txt")
    <title>Физические лица</title>
</head>
<body>
    #parse("common/navigation.vm")

<div class="contentwrap">
    Физические лица
    <br/>
    <img src="/documents/static/images/persons64.png" alt="Физические лица">
    <form method="get" action="/documents/persons/-1">
        <input type="submit" value="Добавить"/>
    </form>
    <table class="list">
        #foreach ($person in $persons)
            <tr>
                <td><a href="#springUrl("/persons/$person.personId")">$person.fullName [$person.personId]</a></td>
            </tr>
        #end
    </table>
</div>
</body>
</html>